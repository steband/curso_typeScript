{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Nest App",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
          "run-script",
          "start:dev"
        ],
        "restart": true,
        "timeout": 30000,
        "sourceMaps": true,
        "outFiles": [],
        "cwd": "${workspaceFolder}",
        "protocol": "inspector"
      }
    ]
  }